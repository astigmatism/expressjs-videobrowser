{
	"mediaRoot": "public/media",
	"thumbRoot": "public/thumbs",
	"webThumbRoot": "/thumbs",
	"webMediaRoot": "/media",
	"previewFilename": ".preview-manifest",
	"images": {
		"prefix": "{{i}}",
		"ext": "png"
	},
	"videos": {
		"prefix": "{{v}}",
		"ext": "png"
	},
	"patterns": {
		"image": "\\.(jpe?g|png|gif|bmp)$",
		"video": "\\.(avi|wmv|mp4|m4v|mpg|mkv|mov|flv)$",
		"avoid": "\\.(db|zip|txt|url)$"
	},
	"videoThumbSize": {
		"width": 300
	},
	"framesPerAxis": 5,
	"handbrakecli": "--encoder x264 -q 22 --two-pass --optimize -x level=4.1:ref=4:b-adapt=2:direct=auto:me=umh:subq=8:rc-lookahead=50:psy-rd=1.0,0.15:deblock=-1,-1:vbv-bufsize=30000:vbv-maxrate=40000:slices=4"
}